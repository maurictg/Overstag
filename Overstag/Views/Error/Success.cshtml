@model string[]
@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Error!</title>
    <link rel="stylesheet" type="text/css" href="~/css/overstag-materialize.min.css" />
    <script src="~/js/jquery-3.4.1.min.js"></script>
    <script src="~/js/materialize.min.js"></script>
    <script src="~/js/Overstag.js"></script>
</head>
<body>
    <nav>
        <div class="nav-wrapper green">
            <a href="/" class="brand-logo center">Gelukt!</a>
        </div>
    </nav>
    <div class="center-align">
        @if (Model.Length > 0)
        {
            <h1><b class="green-text">@Html.Raw(Model[0])</b></h1>
        }
        <br /><br />
        @if (Model.Length > 1)
        {
            <h4><b class="grey-text text-darken-2">@Html.Raw(Model[1])</b></h4>
        }
        <br />
        @if(Model.Length > 2)
        {
            @Html.Raw(Model[2]);
        }
    </div>
    <br /><br />
</body>
</html>
